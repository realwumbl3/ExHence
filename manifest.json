{
  "manifest_version": 3,
  "name": "Exhentai-Ctrl. by wumbl3.",
  "version": "0.01",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://exhentai.org/*"
      ],
      "css": [
        "css.css"
      ],
      "js": [
        "zyx-Lite.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "*://exhentai.org/*"
      ],
      "js": [
        "main.js"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {}
}